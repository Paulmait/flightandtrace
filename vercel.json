{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/fuel/estimate",
      "dest": "/api/fuel/estimate.py"
    },
    {
      "src": "/api/flights/(.*)",
      "dest": "/api/flights.py"
    },
    {
      "src": "/api/subscription/(.*)",
      "dest": "/api/subscription.py"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/admin.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "FUEL_ESTIMATES_ENABLED": "true",
    "API_RATE_LIMIT": "100",
    "PYTHON_VERSION": "3.9"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "regions": ["iad1"],
  "github": {
    "enabled": true
  }
}