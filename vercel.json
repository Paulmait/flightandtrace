{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "framework": null,
  "routes": [
    {
      "src": "/api/health",
      "dest": "/backend/api/health.js"
    },
    {
      "src": "/api/test-env",
      "dest": "/backend/api/test_env.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}