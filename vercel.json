{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/firebase-config",
      "dest": "/api/firebase_config.py"
    },
    {
      "src": "/api/fuel/estimate",
      "dest": "/api/fuel/estimate.py"
    },
    {
      "src": "/api/flights/(.*)",
      "dest": "/api/flights.py"
    },
    {
      "src": "/api/flights-live/(.*)",
      "dest": "/api/flights_live.py"
    },
    {
      "src": "/api/subscription/(.*)",
      "dest": "/api/subscription.py"
    },
    {
      "src": "/api/email/(.*)",
      "dest": "/api/email_service.py"
    },
    {
      "src": "/api/weather/(.*)",
      "dest": "/api/weather.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/api/test-env",
      "dest": "/api/test_env.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(svg|ico|png|jpg|jpeg|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/logo.svg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "FUEL_ESTIMATES_ENABLED": "true",
    "API_RATE_LIMIT": "100",
    "PYTHON_VERSION": "3.9"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "regions": ["iad1"],
  "github": {
    "enabled": true
  }
}