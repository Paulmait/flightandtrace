{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "frontend/build/**",
      "use": "@vercel/static"
    },
    {
      "src": "backend/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/fuel/estimate",
      "dest": "/backend/api/fuel/estimate.js"
    },
    {
      "src": "/api/flights/(.*)",
      "dest": "/backend/api/flights.js"
    },
    {
      "src": "/api/flights-live/(.*)",
      "dest": "/backend/api/flights_live.js"
    },
    {
      "src": "/api/subscription/(.*)",
      "dest": "/backend/api/subscription.js"
    },
    {
      "src": "/api/email/(.*)",
      "dest": "/backend/api/email_service.js"
    },
    {
      "src": "/api/weather/(.*)",
      "dest": "/backend/api/weather.js"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/backend/api/admin.js"
    },
    {
      "src": "/api/health",
      "dest": "/backend/api/health.js"
    },
    {
      "src": "/api/test-env",
      "dest": "/backend/api/test_env.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\.(svg|ico|png|jpg|jpeg|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/logo.svg",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/svg+xml"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "env": {
    "FUEL_ESTIMATES_ENABLED": "true",
    "API_RATE_LIMIT": "100",
    "NODE_VERSION": "18.x"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "regions": ["iad1"],
  "github": {
    "enabled": true
  }
}