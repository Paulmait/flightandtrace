{
  "name": "flighttrace",
  "version": "2.0.0",
  "description": "Real-time flight tracking with fuel estimation and carbon footprint analysis",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:compliance": "playwright test tests/compliance-flows.test.js",
    "test:e2e:legal": "playwright test tests/legal-pages.test.js",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "generate:store-assets": "node scripts/generate-store-assets.js",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:migrate": "supabase db push",
    "supabase:reset": "supabase db reset",
    "vercel:env": "vercel env pull .env.local",
    "postinstall": "node scripts/postinstall.js || true"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/geolocation": "^3.1.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-native": "0.72.7",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-maps": "1.10.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^14.0.0",
    "react-native-vector-icons": "^10.0.2",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@playwright/test": "^1.55.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/react-native": "^12.4.1",
    "@types/jest": "^29.5.10",
    "@types/react": "^18.2.42",
    "@types/react-native": "^0.72.7",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.76.8",
    "playwright": "^1.40.1",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.3.2",
    "vercel": "^32.7.2"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/flighttrace.git"
  },
  "keywords": [
    "flight-tracking",
    "aviation",
    "fuel-estimation",
    "carbon-footprint",
    "react-native",
    "supabase",
    "vercel"
  ],
  "author": "FlightTrace Team",
